{
  "manifest_version": 3,
  "name": "YouTube Speed Buttons",
  "version": "1.0",
  "description": "Adds speed control buttons to YouTube videos.",
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
